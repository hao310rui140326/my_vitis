{
    "name": "Xilinx GZIP HBM Application", 
    "description": [
        "GZip application for HBM platforms resides in `L3/demos/gzip_hbm` directory, it also supports Zlib refer the CLI options."
   ],
    "flow": "vitis",
    "platform_blacklist": [
        "u200",
        "u250",
        "vck"
    ], 
    "platform_whitelist": [
        "u50", 
        "u280"
    ], 
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -t PROJECT/sample.txt",
            "name": "generic launch for all flows"
        }
    ], 
    "post_launch": [
        {
            "launch_cmd": [
                  "./run.sh HOST_EXE LIB_DIR $(BINARY_CONTAINERS)"
             ]
        }
    ], 
    "match_ini": "false", 
    "host": {
        "host_exe": "xgzip", 
        "compiler": {
            "sources": [
                "./src/host.cpp",
                "LIB_DIR/L3/src/zlib_old.cpp",
                "LIB_DIR/common/libs/xcl2/xcl2.cpp",
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp",
                "LIB_DIR/common/libs/logger/logger.cpp",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c",
                "LIB_DIR/common/thirdParty/zlib-1.2.7/adler32.c"
            ],
            "includepaths": [
                "LIB_DIR/L3/include",
                "LIB_DIR/L1/include/hw"
            ],
            "options": "-O3 ",
            "symbols": [
                "C_COMPUTE_UNIT=6",
                "D_COMPUTE_UNIT=8",
                "PARALLEL_BLOCK=4",
                "USE_SINGLE_KERNEL_ZLIBC",
                "ENABLE_HW_CHECKSUM" 
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.cfg", 
            "PROJECT/hls_directives.tcl"
        ], 
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/tests/src/",
                "LIB_DIR/../security/L1/include"
            ],
            "symbols": [
                "PARALLEL_BLOCK=4",
                "MULTIPLE_BYTES=8"
            ], 
            "clflags": [
                "--config PROJECT/advanced.cfg"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress_decompress", 
            "ldclflags": "--kernel_frequency 250 --config PROJECT/opts.cfg", 
            "accelerators": [
                {
                    "name": "xilZlibCompressFull", 
                    "location": "LIB_DIR/L2/src/zlib_compress_multi_engine_mm.cpp", 
                    "num_compute_units": "6", 
                    "compute_units": [
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[10]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[10]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[10]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[11]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[11]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[11]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[12]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[12]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[12]"
                                }
                            ]
                        },
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                    "memory": "HBM[13]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[13]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[13]"
                                }
                            ]
                        }, 
                        {
                            "slr": "SLR1", 
                            "arguments": [
                                {
                                    "name": "in", 
                                   "memory": "HBM[14]"
                                }, 
                                {
                                    "name": "out", 
                                    "memory": "HBM[14]"
                                }, 
                                {
                                    "name": "compressd_size", 
                                    "memory": "HBM[14]"
                                }
                            ]
