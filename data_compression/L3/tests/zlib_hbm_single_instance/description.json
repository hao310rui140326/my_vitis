{
    "name": "Xilinx FPGA Based ZLIB HBM Single Instance", 
    "description": [
        "ZLIB application for HBM resides in `L3/tests/zlibc_hbm_single_instance` directory."
    ], 
    "flow": "vitis", 
    "platform_blacklist": [
        "u250", 
        "vck", 
        "u200"
    ], 
    "platform_whitelist": [
        "u50", 
        "u280"
    ], 
    "launch": [
        {
            "cmd_args": "-sx BUILD/compress_decompress.xclbin -v PROJECT/sample.txt", 
            "name": "generic launch for all flows"
        }
    ], 
    "match_ini": "false", 
    "host": {
        "host_exe": "xil_zlib", 
        "compiler": {
            "sources": [
                "./src/host.cpp", 
                "LIB_DIR/L3/src/zlib_old.cpp", 
                "LIB_DIR/common/libs/xcl2/xcl2.cpp", 
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp", 
                "LIB_DIR/common/libs/logger/logger.cpp", 
                "LIB_DIR/common/thirdParty/zlib-1.2.7/crc32.c", 
                "LIB_DIR/common/thirdParty/zlib-1.2.7/adler32.c"
            ], 
            "includepaths": [
                "LIB_DIR/L3/include", 
                "LIB_DIR/L1/include/hw",
                "LIB_DIR/common/thirdParty/zlib-1.2.7"
            ], 
            "options": "-O3 ", 
            "symbols": [
                "PARALLEL_BLOCK=4", 
                "USE_SINGLE_KERNEL_ZLIBC", 
                "ENABLE_HW_CHECKSUM"
            ]
        }
    }, 
    "v++": {
        "build_datafiles": [
            "PROJECT/advanced.cfg", 
            "PROJECT/hls_directives.tcl"
        ], 
        "compiler": {
            "includepaths": [
                "LIB_DIR/L2/tests/src/", 
                "LIB_DIR/../security/L1/include"
            ], 
            "symbols": [
                "PARALLEL_BLOCK=4", 
                "MULTIPLE_BYTES=8"
            ], 
            "clflags": [
                "--config PROJECT/advanced.cfg"
            ]
        }
    }, 
    "containers": [
        {
            "name": "compress_decompress", 
            "ldclflags": "--kernel_frequency 250 --profile_kernel data:all:all:all --trace_memory HBM[0] --config PROJECT/opts.cfg", 
            "accelerators": [
                {
                    "name": "xilZlibCompressFull", 
                    "location": "LIB_DIR/L2/src/zlib_compress_multi_engine_mm.cpp"
                }, 
                {
                    "name": "xilChecksum32", 
                    "location": "LIB_DIR/L2/src/checksum_mm.cpp"
                }, 
                {
                    "name": "xilZlibDmReader", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_rd.cpp"
                }, 
                {
                    "name": "xilZlibDmWriter", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_wr.cpp"
                }, 
                {
                    "name": "xilDecompressFull", 
                    "location": "LIB_DIR/L2/src/zlib_parallelbyte_decompress_full.cpp"
                }
            ]
        }
    ], 
    "output_files": "sample.txt.*", 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 1000, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 60, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw_build", 
            "vitis_hw_run"
        ], 
        "category": "canary"
    }
}
