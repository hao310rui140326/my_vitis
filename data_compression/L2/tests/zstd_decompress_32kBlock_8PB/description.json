{
    "name": "Xilinx ZSTD Decompress 32KB Block 8 PB", 
    "description": [
        "Xilinx ZSTD Decompression for 32KB block with 8 Byte data width"
    ], 
    "flow": "vitis", 
    "platform_blacklist": [
    ], 
    "platform_whitelist": [
        "u200", 
        "u250", 
        "u50", 
        "u280"
    ], 
    "launch": [
        {
            "cmd_args": "-dx BUILD/xilZstdDecompressStream.xclbin -d PROJECT/sample.txt.zst", 
            "name": "generic launch for all flows"
        }
    ], 
    "post_launch": [
        {
            "launch_cmd": [
                  "./run.sh HOST_EXE LIB_DIR BUILD/xilZstdDecompressStream.xclbin"
             ]
        }
    ], 
    "host": {
        "host_exe": "xil_zstd", 
        "compiler": {
            "sources": [
                "./src/host.cpp", 
                "LIB_DIR/L2/tests/src/zstd.cpp", 
                "LIB_DIR/common/libs/xcl2/xcl2.cpp", 
                "LIB_DIR/common/libs/cmdparser/cmdlineparser.cpp", 
                "LIB_DIR/common/libs/logger/logger.cpp" 
            ], 
            "includepaths": [
                "LIB_DIR/L2/include", 
                "LIB_DIR/L1/include/hw", 
                "LIB_DIR/L2/tests/src"
            ], 
            "symbols": [
                "MULTIPLE_BYTES=8", 
                "ZSTD_BLOCK_SIZE_KB=32" 
            ]
        }
    }, 
    "v++": {
        "compiler": {
            "symbols": [
                "INZSTD=1", 
                "MULTIPLE_BYTES=8", 
                "ZSTD_BLOCK_SIZE_KB=32" 
            ]
        }
    }, 
    "containers": [
        {
            "name": "xilZstdDecompressStream", 
            "ldclflags": "--config PROJECT/opts.cfg", 
            "accelerators": [
                {
                    "name": "xilZlibDmReader", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_rd.cpp"
                }, 
                {
                    "name": "xilZlibDmWriter", 
                    "location": "LIB_DIR/L2/tests/src/zlib_dm_multibyte_wr.cpp"
                }, 
                {
                    "name": "xilZstdDecompressStream", 
                    "location": "LIB_DIR/L2/src/zstd_decompress_stream.cpp"
                }
            ]
        }
    ], 
    "output_files": "sample.txt.zst.*", 
    "testinfo": {
        "disable": false, 
        "jobs": [
            {
                "index": 0, 
                "dependency": [], 
                "env": "", 
                "cmd": "", 
                "max_memory_MB": {
                    "vitis_hw_build": 40960, 
                    "vitis_hw_emu": 28672, 
                    "vitis_sw_emu": 10240, 
                    "vitis_hw_run": 10240
                }, 
                "max_time_min": {
                    "vitis_hw_build": 470, 
                    "vitis_hw_emu": 300, 
                    "vitis_sw_emu": 25, 
                    "vitis_hw_run": 10
                }
            }
        ], 
        "targets": [
            "vitis_sw_emu", 
            "vitis_hw_emu", 
            "vitis_hw"
        ], 
        "category": "canary"
    }
}
